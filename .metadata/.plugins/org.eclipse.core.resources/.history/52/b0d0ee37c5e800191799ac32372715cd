package com.web.bran.factory;

public class DatabaseFactory {

public static Database createDatabase(String vendor) {
		
		Database db = null;
				
		switch(DB.valueOf(vendor)) {
		case ORACLE:
			db = new Oracle();
			break;
		case MARIADB:
			break;
		case MYSQL:
			break;
		}
		
		return db;
		
	}
}
